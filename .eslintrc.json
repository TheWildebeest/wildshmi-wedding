{
  "env": { "browser": true },
  "extends": ["eslint:recommended", "prettier", "next/core-web-vitals"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-undef": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
